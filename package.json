{
  "name": "traider-v1",
  "version": "1.0.0-alpha",
  "description": "Institutional-Grade Autonomous Crypto Trading Platform",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:performance": "vitest run tests/performance",
    "test:security": "vitest run tests/security",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:performance",
    "test:ci": "vitest run --coverage --reporter=json --reporter=html",
    "docs:generate": "typedoc",
    "docs:validate": "tsx scripts/validate-docs.ts",
    "docs:deploy": "npm run docs:generate && npm run docs:validate",
    "security:scan": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    "benchmark:frontend": "tsx scripts/benchmark-frontend.ts",
    "benchmark:trading": "tsx scripts/benchmark-trading.ts",
    "docs:api": "typedoc",
    "docs:serve": "cd docs && python -m http.server 8080",
    "docs:build": "npm run docs:generate && npm run docs:api",
    "docs:coverage": "tsx scripts/validate-docs.ts --coverage-only",
    "docs:watch": "nodemon --watch app --ext ts,tsx --exec 'npm run docs:generate'",
    "adr:new": "tsx scripts/create-adr.ts",
    "adr:list": "ls -la docs/adr/",
    "precommit:check": "tsx scripts/pre-commit-checks.ts",
    "commit": "git-cz",
    "prepare": "husky install"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "next": "^14.0.0",
    "next-auth": "^4.24.11",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "git-cz": "^4.9.0",
    "glob": "^10.0.0",
    "happy-dom": "^18.0.1",
    "husky": "^8.0.0",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.0.0",
    "playwright": "^1.40.0",
    "postcss": "^8.4.32",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.3.6",
    "tsx": "^4.0.0",
    "typedoc": "^0.25.0",
    "typedoc-plugin-markdown": "^3.17.0",
    "typedoc-plugin-mermaid": "^1.11.0",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/traider.git"
  },
  "keywords": [
    "trading",
    "cryptocurrency",
    "autonomous",
    "institutional",
    "nextjs",
    "typescript",
    "fastapi",
    "postgresql"
  ],
  "author": "TRAIDER Team",
  "license": "UNLICENSED"
}
