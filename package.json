{
  "name": "traider-v1",
  "version": "1.0.0-alpha",
  "description": "Institutional-Grade Autonomous Crypto Trading Platform",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "node scripts/build-workaround.mjs",
    "build:next": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:performance": "vitest run tests/performance",
    "test:security": "vitest run tests/security",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:performance",
    "test:ci": "vitest run --coverage --reporter=json --reporter=html",
    "test:coverage:enforce": "vitest run --coverage --reporter=json && node scripts/enforce-coverage.js",
    "docs:generate": "typedoc",
    "docs:validate": "tsx scripts/validate-docs.ts",
    "docs:deploy": "npm run docs:generate && npm run docs:validate",
    "security:scan": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    "security:scan:comprehensive": "npm run security:audit && npm run security:semgrep && npm run security:eslint-security",
    "security:audit": "npm audit --audit-level=moderate --json > security-reports/npm-audit.json",
    "security:audit:ci": "audit-ci --moderate",
    "security:semgrep": "semgrep --config=auto --json --output=security-reports/semgrep.json app/",
    "security:eslint-security": "eslint app/ --ext .ts,.tsx --config .eslintrc.security.js --format json --output-file security-reports/eslint-security.json",
    "security:bandit": "cd backend && bandit -r . -f json -o ../security-reports/bandit.json",
    "security:safety": "cd backend && safety check --json --output ../security-reports/safety.json",
    "security:pip-audit": "cd backend && pip-audit --format=json --output=../security-reports/pip-audit.json .",
    "security:trivy": "trivy fs --scanners vuln,secret,config,license --format table --severity MEDIUM,HIGH,CRITICAL .",
    "security:trivy:secrets": "trivy fs --scanners secret --format table .",
    "security:trivy:vuln": "trivy fs --scanners vuln --format table --severity MEDIUM,HIGH,CRITICAL .",
    "security:trivy:config": "trivy fs --scanners config --format table --severity MEDIUM,HIGH,CRITICAL .",
    "security:trivy:json": "trivy fs --scanners vuln,secret,config --format json --output security-reports/trivy-results.json .",
    "quality:sonar": "sonar-scanner",
    "quality:qlty": "qlty check",
    "quality:qlty:init": "qlty init",
    "quality:qlty:format": "qlty fmt",
    "setup:qlty": "powershell -ExecutionPolicy Bypass -File scripts/setup-qlty.ps1",
    "setup:tokens": "powershell -ExecutionPolicy Bypass -File scripts/setup-security-tokens.ps1",
    "setup:tokens:interactive": "powershell -ExecutionPolicy Bypass -File scripts/setup-security-tokens.ps1 -Interactive",
    "setup:tokens:validate": "powershell -ExecutionPolicy Bypass -File scripts/setup-security-tokens.ps1 -ValidateOnly",
    "setup:tokens:generate": "powershell -ExecutionPolicy Bypass -File scripts/setup-security-tokens.ps1 -GeneratePasswords",
    "validate:setup": "powershell -ExecutionPolicy Bypass -File scripts/validate-setup.ps1",
    "validate:local": "powershell -ExecutionPolicy Bypass -File scripts/validate-setup.ps1 -CheckLocal",
    "validate:secrets": "powershell -ExecutionPolicy Bypass -File scripts/validate-setup.ps1 -CheckSecrets",
    "validate:workflow": "powershell -ExecutionPolicy Bypass -File scripts/validate-setup.ps1 -CheckWorkflow",
    "quality:complexity": "tsx scripts/complexity-analysis.ts",
    "quality:duplication": "jscpd app/ --reporters json --output quality-reports/",
    "quality:all": "npm run quality:complexity && npm run quality:duplication && npm run quality:qlty && npm run test:coverage:enforce",
    "benchmark:frontend": "tsx scripts/benchmark-frontend.ts",
    "benchmark:trading": "tsx scripts/benchmark-trading.ts",
    "docs:api": "typedoc",
    "docs:serve": "cd docs && python -m http.server 8080",
    "docs:build": "npm run docs:generate && npm run docs:api",
    "docs:coverage": "tsx scripts/validate-docs.ts --coverage-only",
    "docs:watch": "nodemon --watch app --ext ts,tsx --exec 'npm run docs:generate'",
    "adr:new": "tsx scripts/create-adr.ts",
    "adr:list": "ls -la docs/adr/",
    "precommit:check": "tsx scripts/pre-commit-checks.ts",
    "commit": "git-cz",
    "prepare": "husky install"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "decimal.js": "^10.5.0",
    "next": "^15.3.4",
    "next-auth": "^4.24.11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.30.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/decimal.js": "^0.0.32",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.0.10",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "audit-ci": "^7.1.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.30.0",
    "eslint-config-next": "^15.3.4",
    "eslint-plugin-security": "^3.0.1",
    "git-cz": "^4.9.0",
    "glob": "^10.4.5",
    "happy-dom": "^15.11.6",
    "husky": "^9.1.7",
    "js-yaml": "^4.1.0",
    "jscpd": "^4.0.5",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.0.0",
    "playwright": "^1.40.0",
    "postcss": "^8.4.32",
    "prettier": "^3.6.2",
    "sonarqube-scanner": "^4.2.0",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.3",
    "typedoc": "^0.28.6",
    "typedoc-plugin-markdown": "^4.7.0",
    "typescript-eslint": "^8.35.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "overrides": {
    "esbuild": "^0.25.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/traider.git"
  },
  "keywords": [
    "trading",
    "cryptocurrency",
    "autonomous",
    "institutional",
    "nextjs",
    "typescript",
    "fastapi",
    "postgresql"
  ],
  "author": "TRAIDER Team",
  "license": "UNLICENSED"
}
